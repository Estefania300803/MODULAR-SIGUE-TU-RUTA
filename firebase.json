{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      { "source": "/firebase-config", "function": "app" },
      { "source": "/recibirGPS",      "function": "app" },
      { "source": "/app{,/**}",       "function": "app" },
      { "source": "**", "destination": "/index.html" }
    ]
  },
  "functionDefaults": {
    "platform": "gcfv1",           
    "runtime": "nodejs18"         
  },
  "functions": [
    {
      "codebase": "default",
      "source": "functions"
    }
  ]
}
